  "webServices": {
    "status": {
      "verbs": [
        "get"
      ],
      "mockFile": "status.json"
    },
    "search": {
      "verbs": [
        "post"
      ],
      "mockFile": "searchList.json"
    },
    "init": {
      "verbs": [
        "post"
      ],
      "mockFile": "error.json"
    },
    "login": {
      "verbs": [
        "post"
      ],
